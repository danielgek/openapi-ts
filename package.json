{
  "name": "openapi-ts",
  "version": "0.2.3",
  "description": "openapi-ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build-release": "tsc -p tsconfig.release.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "node ./lib/cli/cli.js -i ./test/api.json -o ./test/gen.d.ts"
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "commander": "^6.0.0",
    "prettier": "^2.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7"
  },
  "files": [
    "lib/**/*"
  ],
  "bin": {
    "openapi-ts": "./lib/cli/cli.js"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.0.1"
  }
}
